# 필수 환경 변수 - 실제 .env 파일에 설정하세요

# Finnhub API Key (https://finnhub.io에서 발급)
FINNHUB_API_KEY=your_finnhub_api_key_here

# Airflow 설정
AIRFLOW_ADMIN_PASSWORD=secure_admin_password_here
AIRFLOW__WEBSERVER__SECRET_KEY=your_32_char_secret_key_here_123456

# 추가 보안 설정 (선택사항)
AIRFLOW__CORE__FERNET_KEY=your_fernet_key_here

# 주식 데이터 수집 설정
# 종목 선택 방식: manual(수동), trending(트렌딩), sp500(S&P500 상위)
STOCK_SELECTION_METHOD=manual

# 수동 모드일 때 수집할 종목들 (쉼표로 구분)
STOCK_SYMBOLS=TSLA,NVDA,AMD,QCOM,INTC,AAPL,GOOGL,MSFT

# 데이터 수집 주기 설정 (초)
REQUEST_INTERVAL_SECONDS=30